#include <tunables/global>

/home/isisuser/testsource flags=(attach_disconnected) {
  # block IPv4 access
  deny network inet,
  # block IPv6 access
  deny network inet6,
  # Deny raw sockets
  deny network raw,

  # Dissable writing and creating files
  deny /* w,
  /usr/local/lib/perl r,
}
